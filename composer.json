  "minimum-stability": "stable",
  "require": {
    "composer/installers": "^1.9",
    "gigya/php-sdk": "^3.0",
    "ext-json": "*",
    "ext-openssl": "*"
  },
  "repositories": [
    {
      "type": "vcs",
